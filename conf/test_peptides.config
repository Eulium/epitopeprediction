/*
 * -------------------------------------------------
 *  Nextflow config file for running tests
 * -------------------------------------------------
 * Defines bundled input files and everything required
 * to run a fast and simple test. Use as follows:
 *   nextflow run nf-core/epitopeprediction -profile test_peptides
 */

params {
  max_cpus = 2
  max_memory = 6.GB
  max_time = 48.h

  // Input data
  peptides = 'https://github.com/nf-core/test-datasets/raw/epitopeprediction/testdata/peptides/peptides.tsv'
  alleles = 'https://github.com/nf-core/test-datasets/raw/epitopeprediction/testdata/alleles/alleles.txt'
}
